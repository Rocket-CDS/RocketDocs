<?xml version="1.0" encoding="utf-8" ?>
<root>
  <sqlfilter>
    <article>
      and (
      isnull([XMLData].value('(genxml/lang/genxml/textbox/articlename)[1]','nvarchar(max)'),'') like '%{searchtext}%'
      or isnull([XMLData].value('(genxml/textbox/articleref)[1]','nvarchar(max)'),'') like '%{searchtext}%'
      or isnull([XMLData].value('(genxml/lang/genxml/textbox/articlekeywords)[1]','nvarchar(max)'),'') like '%{searchtext}%'
      )
    </article>    
  </sqlfilter>
  <sqlorderby>
    <article>
      <name>
        order by RLangIdx.XmlData.value('(genxml/lang/genxml/textbox/articlename)[1]','nvarchar(max)')
      </name>
      <name-desc>
        order by RLangIdx.XmlData.value('(genxml/lang/genxml/textbox/articlename)[1]','nvarchar(max)') desc
      </name-desc>
      <ref>
        order by RLangIdx.XmlData.value('(genxml/textbox/articleref)[1]','nvarchar(max)')
      </ref>
      <ref-desc>
        order by RLangIdx.XmlData.value('(genxml/textbox/articleref)[1]','nvarchar(max)') desc
      </ref-desc>
    </article>
  </sqlorderby>

